<odoo>
    <data>

        <record id="action_garage_vehicle_service" model="ir.actions.act_window">
            <field name="name">Vehicle Services</field>
            <field name="res_model">garage.vehicle.service</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record id="action_garage_consumable_usage" model="ir.actions.act_window">
            <field name="name">Consumable Usage</field>
            <field name="res_model">garage.consumable.usage</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record id="action_garage_consumable_report" model="ir.actions.act_window">
            <field name="name">Monthly Consumption Report</field>
            <field name="res_model">garage.consumable.report</field>
            <field name="view_mode">tree</field>
        </record>
        <record id="action_garage_job_card_part_tree" model="ir.actions.act_window">
            <field name="name">Part Request Report</field>
            <field name="res_model">garage.job.card.part</field>
            <field name="view_mode">tree</field>
            <field name="context">{}</field>
            <field name="domain">[]</field>
        </record>


        <menuitem id="menu_job_card_root" name="Manage Job Cards" parent="fleet.menu_root"/>

        <menuitem id="menu_job_card" name="Job Cards" parent="menu_job_card_root" action="action_job_card"/>
        <menuitem id="menu_garage_job_card_part_report"
                  name="Part Request Report"
                  parent="menu_job_card_root"
                  action="action_garage_job_card_part_tree"
        />
        <menuitem id="menu_sublet_work_report"
              name="Sublet Cost Report"
              parent="menu_job_card_root"
              action="action_sublet_work_report"/>

    </data>
</odoo>

