<odoo>
    <template id="report_gatch_supply_template">
        <t t-call="web.external_layout">
            <t t-foreach="docs" t-as="doc">
                <main class="o_report_layout_standard">
                    <div class="page">
                        <h2 style="text-align:center;">Gatch Supply Report</h2>
                        <table class="table table-sm">
                            <tbody>
                                <tr>
                                    <td><strong>Date</strong></td>
                                    <td><span t-esc="doc.date"/></td>
                                </tr>
                                <tr>
                                    <td><strong>Trip Ticket No</strong></td>
                                    <td><span t-esc="doc.trip_ticket_no"/></td>
                                </tr>
                                <tr>
                                    <td><strong>Plate No</strong></td>
                                    <td><span t-esc="doc.license_plate.name"/></td>
                                </tr>
                                <tr>
                                    <td><strong>Driver</strong></td>
                                    <td><span t-esc="doc.driver_name.name"/></td>
                                </tr>
                                <tr>
                                    <td><strong>Description</strong></td>
                                    <td><span t-esc="doc.description"/></td>
                                </tr>
                                <tr>
                                    <td><strong>Qty (M3)</strong></td>
                                    <td><span t-esc="doc.qty_m3"/></td>
                                </tr>
                                <tr>
                                    <td><strong>Location</strong></td>
                                    <td><span t-esc="doc.location"/></td>
                                </tr>
                                <tr>
                                    <td><strong>Invoice No</strong></td>
                                    <td><span t-esc="doc.invoice_no.name"/></td>
                                </tr>
                                <tr>
                                    <td><strong>Invoice For</strong></td>
                                    <td><span t-esc="doc.invoice_for"/></td>
                                </tr>
                                <tr>
                                    <td><strong>Bill To</strong></td>
                                    <td><span t-esc="doc.bill_to"/></td>
                                </tr>
                                <tr>
                                    <td><strong>Month</strong></td>
                                    <td><span t-esc="doc.month"/></td>
                                </tr>
                                <tr>
                                    <td><strong>Service Type</strong></td>
                                    <td><span t-esc="doc.service_type_id.name"/></td>
                                </tr>
                                <tr>
                                    <td><strong>Status</strong></td>
                                    <td><span t-esc="doc.state"/></td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </main>
            </t>
        </t>
    </template>
</odoo>
