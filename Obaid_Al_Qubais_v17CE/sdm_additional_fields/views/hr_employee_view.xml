<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_employee_form_inherit_custom" model="ir.ui.view">
        <field name="name">hr.employee.form.inherit.custom</field>
        <field name="model">hr.employee</field>
        <field name="inherit_id" ref="hr.view_employee_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='mobile_phone']" position="before">
                <field name="hrmsempid"/>
            </xpath>
            <xpath expr="//field[@name='work_email']" position="after">
                <field name="vacpreyear"/>
                <field name="vaccuryear"/>
            </xpath>
            <xpath expr="//field[@name='coach_id']" position="after">
                <field name="usersign"/>
                <field name="emp_active"/>
            </xpath>
            <xpath expr="//field[@name='hrmsempid']" position="after">
                <field name="extempNo"/>
                <field name="u_emid"/>
            </xpath>
        </field>
    </record>
</odoo>